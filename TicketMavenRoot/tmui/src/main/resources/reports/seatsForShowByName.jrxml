	<queryString>
		<![CDATA[SELECT seats.*, seatmapping.seat_number, shows.layout as slay, shows.record_id as shrec, customers.last_name, customers.first_name, customers.special_needs, 
SUBSTRING(customers.last_name,1,1) as INIT from seats, shows, seatmapping 
JOIN tickets ON (seats.record_id = tickets.seat_id and tickets.show_id = $P{show_id})
JOIN customers ON (tickets.customer_id = customers.record_id )
WHERE shows.record_id = $P{show_id} AND seats.layout = shows.layout AND seatmapping.seat_id = seats.record_id
order by INIT, customers.last_name, customers.first_name]]>
