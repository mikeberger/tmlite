		<![CDATA[SELECT
     TMTABLES."LABEL" AS TMTABLES_LABEL,
     CUSTOMERS."FIRST_NAME" AS CUSTOMERS_FIRST_NAME,
     CUSTOMERS."LAST_NAME" AS CUSTOMERS_LAST_NAME,
     SHOWS."NAME" AS SHOWS_NAME,
     SHOWS."TIME" AS SHOWS_TIME,
     RESERVATIONS."SHOW_ID" AS RESERVATIONS_SHOW_ID,
     RESERVATIONS."NUM" AS RESERVATIONS_NUM
FROM
     "PUBLIC"."TMTABLES" TMTABLES INNER JOIN "PUBLIC"."RESERVATIONS" RESERVATIONS ON TMTABLES."RECORD_ID" = RESERVATIONS."TABLE_ID"
     INNER JOIN "PUBLIC"."SHOWS" SHOWS ON RESERVATIONS."SHOW_ID" = SHOWS."RECORD_ID"
     INNER JOIN "PUBLIC"."CUSTOMERS" CUSTOMERS ON RESERVATIONS."CUSTOMER_ID" = CUSTOMERS."RECORD_ID"
WHERE
     RESERVATIONS_SHOW_ID = $P{showid}
ORDER BY TMTABLES."LABEL",
CUSTOMERS_LAST_NAME]]>
