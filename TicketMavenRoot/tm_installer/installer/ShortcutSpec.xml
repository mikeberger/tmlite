<izpack:shortcuts version="5.0"
                  xmlns:izpack="http://izpack.org/schema/shortcuts"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://izpack.org/schema/shortcuts http://izpack.org/schema/5.0/izpack-shortcuts-5.0.xsd">	<programGroup defaultName="TicketMaven" location="applications/startMenu" />
	<shortcut name="TicketMaven" target="$INSTALL_PATH/TicketMaven.exe"
		commandLine="" workingDirectory="$INSTALL_PATH" programGroup="yes"
		desktop="yes" applications="no" startMenu="no" startup="no">
		<createForPack name="Base" />
	</shortcut>
	<shortcut name="Uninstall" target="$INSTALL_PATH/Uninstaller/uninstaller.jar"
		workingDirectory="$INSTALL_PATH/Uninstaller" programGroup="yes"
		desktop="no" applications="no" startMenu="no" startup="no"
		runAsAdministrator="true">
		<createForPack name="Base" />
	</shortcut>
</izpack:shortcuts>

